{
  "extends": "standard",
  "plugins": [
    "jest"
  ],
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "space-before-function-paren":["off"]
  },
  "globals": {
    "expect": true,
    "test": true,
    "describe": true
  }
}